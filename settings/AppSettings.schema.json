{
  "$defs": {
    "CNNNetwork": {
      "description": "Enum for CNN network options",
      "enum": [
        "DAN",
        "AHOY",
        "YOLO",
        "CERULEAN"
      ],
      "title": "CNNNetwork",
      "type": "string"
    },
    "ObjectDetection": {
      "description": "Object detection configuration settings",
      "properties": {
        "cnn_network": {
          "$ref": "#/$defs/CNNNetwork",
          "default": "DAN",
          "updatable": true
        },
        "min_object_size": {
          "default": 6,
          "minimum": 0,
          "title": "Min Object Size",
          "type": "integer",
          "updatable": true
        },
        "cnn_confidence_threshold": {
          "default": 0.5,
          "maximum": 1,
          "minimum": 0,
          "title": "Cnn Confidence Threshold",
          "type": "number",
          "updatable": true
        }
      },
      "title": "ObjectDetection",
      "type": "object"
    },
    "Tracker": {
      "description": "Tracker configuration settings",
      "properties": {
        "count_min_seen": {
          "default": 10,
          "minimum": 0,
          "title": "Count Min Seen",
          "type": "integer",
          "updatable": true
        },
        "count_max_unseen": {
          "default": 20,
          "minimum": 0,
          "title": "Count Max Unseen",
          "type": "integer",
          "updatable": true
        }
      },
      "title": "Tracker",
      "type": "object"
    }
  },
  "description": "Main application settings model",
  "properties": {
    "object_detection": {
      "$ref": "#/$defs/ObjectDetection"
    },
    "tracker": {
      "$ref": "#/$defs/Tracker"
    }
  },
  "title": "AppSettings",
  "type": "object"
}